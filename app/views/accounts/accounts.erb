<h1>Account information will display here.</h1>
<% @accounts.each do |account| %>
  <p><label for="account">Account Number: </label><%= account.account_number %></p>
  <p><label for="account_type">Account Type: </label><%= account.account_type %></p>
  <p><label for="balance">Balance: </label><%= account.balance %></p>
<% end %>


